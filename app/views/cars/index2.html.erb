<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>緯線ボタンの例</title>
    <link rel="stylesheet" href="styles.css">
</head>
<body>
    <div class="button-container">
        <div class="button-basic">
            <%= button_to "ベーシック", cars_basic_path %>
        </div>
        <div class="button-basic">
            <%= button_to "ミドル", cars_middle_path %>
        </div>
        <div class="button-basic">
            <%= button_to "プレミアム", cars_premium_path %>
        </div>
        <%= link_to "新規投稿へ", new_car_path %>
        <%= link_to "一覧へ", cars_hajimari_path %>
        <%= link_to "診断を始める", new_carpick_path, class: "link" %>
    </div>

    <div>
      <div class="ryoukinn">
      
        <div class="ryoukinn-b">
        <p10>ベーシック<p10>
        </div>

        <div class="ryoukinn-m">
        <p10>ミドル<p10>
        </div>

        <div class="ryoukinn-p">
        <p10>プレミアム<p10>
        </div>

      </div>
    </div>
</body>